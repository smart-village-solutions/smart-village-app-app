{
  "entryPoints": ["src"],
  "tsconfig": "tsconfig.json",
  "out": "docs/typedoc",
  "cleanOutputDir": true,
  "exclude": [
    "**/__tests__/**",
    "**/__mocks__/**",
    "**/*.test.*",
    "**/*.spec.*",
    "node_modules"
  ],
  "readme": "none",
  "includeVersion": true,
  "excludePrivate": true,
  "excludeProtected": true,
  "excludeExternals": true,
  "hideGenerator": true,
  "disableSources": false,
  "logLevel": "Verbose"
}
